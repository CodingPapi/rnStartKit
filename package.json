{
  "name": "rnStartKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "link": "react-native link",
    "lint": "eslint --ext .js,.jsx,.ts,tsx src ",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts,tsx src ",
